# Top-most EditorConfig file.
root = true

# Unix-style newlines with a newline ending every file.
[*]
end_of_line = lf
insert_final_newline = true

# Config for `.html` files.
[*.html]
charset = utf-8
indent_style = space
indent_size = 2

# Config for `.py` files.
[*.py]
charset = utf-8
indent_style = space
indent_size = 4
